<h1>{{type}} notes</h1>
{{#each notes}}
    <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">{{title}}</span>
          <p>{{body}}</p>
        </div>
        <div>
            <span>Created by: {{author.username}}</span>
        </div>
        <div class="card-action">
          <a href="#">Read more</a>
        </div>
      </div>
    </div>
  </div>
{{else}}
    <p>No note available</p>
{{/each}}